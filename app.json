{
  "expo": {
    "name": "NYgame",
    "scheme": "nygame",
    "slug": "NYgame",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "name": "베이글 럭키 뽑기",
      "shortName": "베이글뽑기",
      "description": "베이글 카페 럭키 드로우 이벤트 앱",
      "themeColor": "#FF6B35",
      "backgroundColor": "#FFF9F5"
    },
    "plugins": [
      "expo-sqlite",
      "expo-router",
      "expo-font"
    ]
  }
}
